    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Pravams_Chat::Chat" title="Chat" sortOrder="10">
                    <resource id="Pravams_Chat::Chat_save" title="Save Chat" sortOrder="10"/>
                    <resource id="Pravams_Chat::Chat_delete" title="Delete Chat" sortOrder="20"/>
                    <resource id="Pravams_Chat::Chat_update" title="Update Chat" sortOrder="30"/>
